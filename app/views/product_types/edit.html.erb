<h1>Editing Product Type</h1>

<% form_for(@product_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @product_type %> |
<%= link_to 'Back', product_types_path %>